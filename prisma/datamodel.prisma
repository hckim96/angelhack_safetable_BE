type User {
  id: ID! @id 
  username: String!
  email: String! @unique
  password: String!
  rooms: [Room]

  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}
type Room {
  id: ID! @id
  name: String! @unique
  owner: User! @relation(link: INLINE)
  question: String! 
  answer: String!

  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}
